{
  "name": "ed_editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run build-deps:dev && concurrently npm:serve:*",
    "serve:editor": "vue-cli-service serve",
    "serve:engine": "watch \"npm run build-engine:dev\" ./engine --interval=0.01",
    "serve:shared": "watch \"npm run build-shared:dev\" ./shared --interval=0.01",
    "build": "npm run build-deps && vue-cli-service build",
    "build-portable": "npm run build-deps && vue-cli-service build --mode portable",
    "build-engine": "node ./scripts/build_engine.js",
    "build-engine:dev": "node ./scripts/build_engine.js --debug",
    "build-shared": "node ./scripts/build_shared.js",
    "build-shared:dev": "node ./scripts/build_shared.js --debug",
    "build-deps": "concurrently \"npm run build-engine\" \"npm run build-shared\"",
    "build-deps:dev": "concurrently \"npm run build-engine:dev\" \"npm run build-shared:dev\"",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "file-saver": "^2.0.2",
    "victor": "^1.1.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.3",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "concurrently": "^7.0.0",
    "rollup": "^2.70.1",
    "uglify-js": "^3.15.3",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-template-compiler": "^2.6.11",
    "watch": "^1.0.2"
  }
}
